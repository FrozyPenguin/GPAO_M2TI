{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs", /* Required for 'import {Article_type, Article, Articles_} from './Articles';' */
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "esnext",
    "strict": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ],
  "include": [
    "./src/**/*.ts"
  ]
}